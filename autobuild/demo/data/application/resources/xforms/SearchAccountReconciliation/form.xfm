                <xforms:value ref="."/>
              </xforms:itemset>
            </xforms:select1>
			<!-- <xforms:output bind="assignedPreparerBind"/> -->
		</xforms:group>
	</td>
<!-- Removed because this is the only field that bogs down the searches and doesn't initially search off account
	<td rowspan="1" colspan="1" style="border: 1px solid black;vertical-align:top;">
		<xforms:group>
			<label style="font-weight: bold">Reconciliation Status</label><br/>
			<xforms:select1 appearance="minimal" selection="closed" id="reconciliatonStatus" bind="reconciliatonStatusBind">
			<xforms:label/>
			<xforms:itemset nodeset="instance('recon_status')/choice">
				<xforms:label ref="@show"></xforms:label>
				<xforms:value ref="."></xforms:value>
			</xforms:itemset>
			</xforms:select1>
		</xforms:group>
	</td>
	<td rowspan="1" colspan="1" style="border: 1px solid black;vertical-align:top;">
		<xforms:group>
			<label style="font-weight: bold">Certification Status</label><br/>
			<xforms:select1 appearance="minimal" selection="closed" id="reconciliatonStatus" bind="reconciliatonStatusBind">
			<xforms:label/>
			<xforms:itemset nodeset="instance('cert_status')/choice">
				<xforms:label ref="@show"></xforms:label>
				<xforms:value ref="."></xforms:value>
			</xforms:itemset>
			</xforms:select1>
		</xforms:group>
	</td>
-->
</tr>
</table>
</xforms:group>
