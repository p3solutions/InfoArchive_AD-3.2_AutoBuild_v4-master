            	<xforms:label ref="@show"></xforms:label>
            	<xforms:value ref="."></xforms:value>
         	  </xforms:itemset>
            </xforms:select1><br/>
            <xforms:select1 appearance="minimal" selection="closed" id="department" bind="departmentBind">
              <xforms:label>Department&#160;</xforms:label>
              <xforms:itemset nodeset="instance('generalPulldownValues')/dept//RECONCILINGDEPT">
            	<xforms:label ref="."></xforms:label>
            	<xforms:value ref="."></xforms:value>
         	  </xforms:itemset>
            </xforms:select1>
            </xforms:group>
          </td>
		  <td colspan="1" rowspan="2" align="center" valign="bottom">
          <xforms:trigger id="input6-0">
			<xforms:label>Submit</xforms:label>
			<xforms:send submission="searchSVHarborAccountModelSubmission"/>
		  </xforms:trigger>
		  </td>
</tr>
<tr>
	<td rowspan="1" colspan="1" style="border: 1px solid black;vertical-align:top;">
		<xforms:group>
