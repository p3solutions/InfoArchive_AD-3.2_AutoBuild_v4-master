            	<xforms:value ref="."></xforms:value>
         	  </xforms:itemset>
			</xforms:select1><br/>
			<xforms:select1 appearance="minimal" selection="closed" id="recDeptRisk" bind="recDeptRiskBind">
			<xforms:label>Rec Department Risk</xforms:label>
              <xforms:itemset nodeset="instance('rec_dept_risk')/choice">
            	<xforms:label ref="@show"></xforms:label>
            	<xforms:value ref="."></xforms:value>
         	  </xforms:itemset>
         	</xforms:select1>
            </xforms:group>
          </td>
          <td rowspan="2" colspan="1" style="border: 1px solid black;vertical-align:top;">
          	<xforms:group>
          	<label style="font-weight: bold">Optional Values</label><br/>
            <xforms:input id="profitCenter" bind="profitCenterBind">
              <xforms:label>Profit Center&#160;</xforms:label>
            </xforms:input><br/>
            <xforms:select1 appearance="minimal" selection="closed" id="businessUnit" bind="businessUnitBind">
