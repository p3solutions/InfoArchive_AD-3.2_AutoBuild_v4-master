<xforms:group model="searchSVHarborAccountModel" id="searchSVHarborAccount">
<table xmlns="http://www.w3.org/1999/xhtml">
<tr>
	<td rowspan="1" colspan="1" style="border: 1px solid black;vertical-align:top;">
    	<xforms:group>
        	<label style="font-weight: bold">Company&#160;</label><br />
            <xforms:select1 appearance="minimal" selection="closed" id="company" bind="companyBind">
              <xforms:label/>
              <xforms:itemset nodeset="instance('company_list')/choice">
            	<xforms:label ref="@show"></xforms:label>
            	<xforms:value ref="."></xforms:value>
         	  </xforms:itemset>
        	</xforms:select1>
    	</xforms:group>
    </td>
	<td rowspan="1" colspan="1" style="border: 1px solid black;vertical-align:top;">
		<xforms:group>
			<label style="font-weight: bold">Posting Acct #</label><br/>
			<table><tr><td align="right">
			<xforms:input id="fromPostingAcct" bind="fromPostingAcctBind">
			<xforms:label>From</xforms:label>
			</xforms:input><br/>
			<xforms:input id="toPostingAcct" bind="toPostingAcctBind">
			<xforms:label>To</xforms:label>
			</xforms:input>
			</td></tr></table>
		</xforms:group>
	</td>

          <td rowspan="2" colspan="1" style="border: 1px solid black;vertical-align:top;">
          	<xforms:group>
          	<label style="font-weight: bold">Risk</label><br/>
